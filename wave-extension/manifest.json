{
   "action": {
      "default_icon": {
         "16": "/img/wave16bk.png",
         "32": "/img/wave32bk.png",
         "64": "/img/wave64bk.png"
      },
      "default_title": "WAVE"
   },
   "background": {
      "service_worker": "service_worker.js"
   },
   "commands": {
      "toggle-extension": {
         "description": "Send a 'toggle-extension' event to the extension",
         "suggested_key": {
            "default": "Ctrl+Shift+U"
         }
      }
   },
   "description": "Evaluate web accessibility within your browser.",
   "host_permissions": [ "file:///*", "http://*/*", "https://*/*" ],
   "icons": {
      "128": "/img/wave128.png",
      "16": "/img/wave16.png",
      "32": "/img/wave32.png",
      "48": "/img/wave48.png",
      "64": "/img/wave64.png",
      "96": "/img/wave96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "WAVE Evaluation Tool",
   "permissions": [ "activeTab", "contextMenus", "scripting" ],
   "short_name": "WAVE",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.2.7.2",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "img/*", "styles/*", "chrome/*", "background.js", "inject.js", "sidebar.html", "wave.min.js", "sidebar.min.js" ]
   } ]
}
